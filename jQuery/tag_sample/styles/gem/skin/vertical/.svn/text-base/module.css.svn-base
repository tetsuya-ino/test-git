}

.tbl-data-02 input,
.tbl-data-02 select{
padding:2px;
}
.tbl-data-02 input.form-btn{
padding:1px 3px;
}

.tbl-data-02 .tbl-inner{
border:1px solid #d5d5d5;
white-space: nowrap;
}
.tbl-data-02 .tbl-inner th{
border:none;
background:#eaeaea;
}
.tbl-data-02 .tbl-inner td{
border:none;
}
.tbl-data-02 .tbl-inner tr.odd{
background:#f9f9f9;
}

.tbl-stripe tr.odd{
