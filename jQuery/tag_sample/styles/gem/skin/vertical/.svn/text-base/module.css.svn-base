}
*:first-child+html div.cal-wrap{
zoom:1
}
*:first-child+html div.cal-wrap-02{
zoom:1
}
*:first-child+html div.cal-wrap-03{
zoom:1
}
.tbl-calendar-03{
table-layout: fixed;
}

.tbl-calendar-03 thead th span.thbox{
margin:-1px 0 0 -1px;
padding:10px 0;
border:1px solid #d5d5d5;
border-bottom:none;
display:block;
background:#f6f6f6;
text-align:center;
zoom:1;
position:relative;
}

.tbl-calendar-03 tbody td{
vertical-align:top;
}

.tbl-calendar-03 .daybox{
margin:-1px 0 0 -1px;
padding:7px 9px 9px 7px;
border:1px solid #d5d5d5;
position:relative;
zoom:1;
height:68px;
}
.tbl-calendar-03 .daybox:hover{
position:relative;
z-index:2;
border:1px solid #999999;
zoom:1;
}
.tbl-calendar-03 td.other .daybox:hover{
border:1px solid #d5d5d5;
}

.tbl-calendar-03 th.holiday,
.tbl-calendar-03 td.holiday{
color:#ab0000;
}

.tbl-calendar-03 th.saturday,
.tbl-calendar-03 td.saturday{
color:#19447d;
}

.tbl-calendar-03 td.other{
color:#999999;
background:#eaeaea;
}

.tbl-calendar-03 .today{
background:#dce9f5 !important;
}

.tbl-calendar-03 .head{
overflow:hidden;
padding-bottom:5px;
}

.tbl-calendar-03 td .date{
float:left;
}
.tbl-calendar-03 td .add{
float:right;
padding-top:3px;
}

.tbl-calendar-03 td .close{
margin-left:10px;
font-size:108%;
float:right;
cursor:pointer;
color:#222222;
display:none;
}

*:first-child+html .tbl-calendar-03 td img{
vertical-align:top;
}

.tbl-calendar-03 ul.schedule li{
overflow:hidden;
font-size:92%;
line-height:1.4;
white-space:nowrap;
}

.tbl-calendar-03 ul.schedule li span.time{
display:block;
}

.tbl-calendar-03 .daybox .daymore{
white-space:nowrap;
overflow:hidden;
bottom:3px;
left:0;
position:absolute;
}

.tbl-calendar-03 .daybox.current{
position:relative;
z-index:5;
}
.tbl-calendar-03 .daybox.current .schedulebox{
border:1px solid #999999;
position:absolute;
top:-1px;
left:-1px;
z-index:50;
width:293px;
/*min-height:215px;*/
background:#FFFFFF;
}
.tbl-calendar-03 .daybox.current .head{
padding:7px 9px 6px 7px;
border-bottom:1px solid #d5d5d5;
background:#f6f6f6;
}

.tbl-calendar-03 .daybox.current .daydetail{
margin:5px 5px 5px 0;
padding:0 0 0 5px;
overflow-x:hidden;
overflow-y:auto;
width:284px;
max-height:200px;
}
.tbl-calendar-03 .daybox.current .daydetail ul li{
margin-top:3px;
padding-top:3px;
border-top:1px dotted #d5d5d5;
overflow:visible;
white-space:normal;
}
.tbl-calendar-03 .daybox.current .daydetail ul li:first-child{
margin-top:0;
padding-top:0;
border-top:none;
}

.tbl-calendar-03 .daybox.current .moreschedule li{
font-size:92%;
line-height:1.4;
}

.tbl-calendar-03 .daybox .moreschedule{
display:none;
}
.tbl-calendar-03 div.daybox.current .moreschedule{
display:block;
}
.tbl-calendar-03 div.daybox.current ul.schedule,
.tbl-calendar-03 div.daybox.current p.daymore{
display:none;
}
.tbl-calendar-03 .daybox .schedule,
.tbl-calendar-03 .daybox .daymore{
display:block;
}
.tbl-calendar-03 div.daybox.current .close{
display:block;
}
.tbl-calendar-03 div.daybox.current .close:hover{
color:#999999;
}

