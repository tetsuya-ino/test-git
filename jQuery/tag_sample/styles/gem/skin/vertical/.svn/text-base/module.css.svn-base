/*  Hdg Style
--------------------------------------------------------- */
.hgroup-01{
margin-bottom:20px;
padding-bottom:4px;
border-bottom:2px solid #d5d5d5;
font-size:150%;
}
.hgroup-01 span{
padding-right:6px;
color:#222222;
font-weight:bold;
}
.hgroup-01 span img{
margin-top:-2px;
vertical-align:middle;
}

.hgroup-02{
margin-bottom:12px;
font-size:117%;
}

.hgroup-03{
margin-bottom:10px;
color:#225d97;
background:url(../../../../images/gem/skin/vertical/bg-hgroup-01.gif) repeat-x left center;
font-size:109%;
}

.hgroup-03 h3 span{
background:#ffffff url(../../../../images/gem/skin/vertical/icon-arrow-01.png) no-repeat left 0.6em;
font-weight:bold;
padding:0 10px 0 15px;
}

.disclosure-close.hgroup-03 h3 span{
background:#ffffff url(../../../../images/gem/skin/vertical/icon-arrow-03.png) no-repeat left 0.55em;
}

.hgroup-03.sechead h3 span{
cursor:pointer;
}

/*  Table Style
--------------------------------------------------------- */
.tbl-data-01{
border:1px solid #d5d5d5;
width:100%;
}
.tbl-data-01 tbody th,
.tbl-data-01 tbody td{
padding:7px;
background:#f9f9f9;
text-align:left;
}
.tbl-data-01 tbody tr.odd th,
.tbl-data-01 tbody tr.odd td{
background:#FFFFFF;
}

.tbl-data-02{
margin-bottom:20px;
border-top:1px solid #d5d5d5;
border-right:1px solid #d5d5d5;
border-bottom:1px solid #d5d5d5;
width:100%;
table-layout:fixed;
}
.tbl-data-02.vertical-table{
border:1px solid #d5d5d5;
}
.tbl-data-02.data-type-01{
border-left:1px solid #d5d5d5;
}

.tbl-data-02 thead th{
padding:9px 7px;
border-top:1px dotted #d5d5d5;
background:#eaeaea;
text-align:left;
vertical-align:middle;
}

.tbl-data-02 tbody th{
border-top:1px dotted #d5d5d5;
border-left:1px solid #d5d5d5;
background:#f6f6f6;
text-align:left;
}
.tbl-data-02.vertical-table thead th{
border-bottom:1px dotted #d5d5d5;
border-left:1px solid #d5d5d5;
background:#f6f6f6;
text-align:left;
}
.tbl-data-02 tbody td{
border-top:1px dotted #d5d5d5;
}
.tbl-data-02 tbody td:first-child,
.tbl-data-02 thead th{
/* border-top:none; */
}
.tbl-data-02 tbody th,
.tbl-data-02 tbody td{
padding:9px 7px;
vertical-align:middle;
}
.tbl-data-02 tbody td.sec-data{
border-left:1px solid #d5d5d5;
}
.tbl-data-02.vertical-table tbody td{
border-left:1px dotted #d5d5d5;
