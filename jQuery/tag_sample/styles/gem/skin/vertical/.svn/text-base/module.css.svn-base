background:-ms-linear-gradient(top, #5f5f5f 0%, #838383 100%) repeat 0 0;
background:linear-gradient(top, #5f5f5f 0%, #838383 100%) repeat 0 0;
color:#ffffff;
text-shadow:none;
font-weight:bold;
}
.tabList-listing-01 li:first-child a,
.tabList-listing-01 li.current:first-child a{
padding-left:5px;
border-top-left-radius:5px;
border-left:none;
}
.tabList-listing-01 p.create{
padding:9px 10px 0 17px;
float:right;
text-shadow:1px 1px 0px #efefef;
}
.tabList-listing-01 p.create a{
color:#383838;
}
.box-listing-01{
padding:0 0 10px;
border:1px solid #d5d5d5;
border-top:0;
border-bottom-left-radius:10px;
border-bottom-right-radius:10px;
width:100%;
background:#f6f6f6;
height: 300px;
}

/*--- お知らせ情報 パスワード警告 ---*/
.info-list p.pw-warn-area {
	margin: 2px 0 20px;
	padding: 7px 0;
}
.info-list p.pw-warn-area a {
	text-decoration:underline;	//リンクがわかりにくいので設定
}
.info-list span.pw-warn-mark {
	margin:0 7px;
	float:left;
}

/*保存アイテム情報*/
div.saved-list-info th{
width:18%;
}
div.saved-list-info td{
width:82%;
}

/* jqtreeの選択色をjqGridに合わせる */
ul.jqtree-tree li.jqtree-selected > .jqtree-element,
ul.jqtree-tree li.jqtree-selected > .jqtree-element:hover {
background: #DFEFFC !important;
}