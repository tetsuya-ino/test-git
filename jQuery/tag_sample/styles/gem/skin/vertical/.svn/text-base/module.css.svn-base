/*カレンダーヘッド*/

.cal-header{
margin-bottom:8px;
overflow:hidden;
}

.cal-header h3,
.cal-header .cal-page-nav,
.cal-header p{
float:left;
}
.cal-header .hgroup-02{
margin-bottom:6px;
}
.cal-header h3{
padding-top:5px;
}
.cal-header h3 span{
font-weight: bold;
}

.cal-page-nav{
margin:0 5px 0 14px;
border-radius:3px;
border:1px solid #c2c2c2;
overflow:hidden;
zoom:1;
width:47px;
background:#e4e4e4;
background:-webkit-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
background:-moz-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
}

.cal-page-nav li{
display:inline;
}

.cal-page-nav li a{
padding-bottom:1px;
float:left;
display:block;
width:22px;
height:23px;
color:#999999;
text-align:center;
text-shadow:1px 1px 0px #ffffff;
font-size:25px;
line-height:1;
font-weight:bold;
zoom:1;
}
.cal-page-nav li.previous a{
border-right:1px solid #c2c2c2;
zoom:1;
}
.cal-page-nav li.next a{
border-left:1px solid #f6f6f6;
zoom:1;
}
.this-month a{
padding:4px 0 5px;
border-radius:3px;
border:1px solid #c2c2c2;
display:block;
width:88px;
height:15px;
background:#e4e4e4;
background:-webkit-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
background:-moz-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
color:#555555;
font-weight:bold;
text-align:center;
text-shadow:1px 1px 0px #ffffff;
}

.this-month a:hover,
.cal-page-nav li a:hover{
background:#f5f5f5;
background:-webkit-linear-gradient(top, #ffffff 0%, #f5f5f5 100%);
background:-moz-linear-gradient(top, #ffffff 0%, #f5f5f5 100%);
text-decoration:none;
}

.cal-content-nav{
float:right;
overflow:hidden;
width:154px;
}
.cal-content-nav li{
float:left;
border-top:1px solid #c2c2c2;
border-bottom:1px solid #c2c2c2;
}
.cal-content-nav li a{
padding:4px 0 5px;
border-right:1px solid #c2c2c2;
border-left:1px solid #f6f6f6;
display:block;
width:49px;
height:15px;
background:#e4e4e4;
background:-webkit-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
background:-moz-linear-gradient(top, #f5f5f5 0%, #e4e4e4 100%);
color:#555555;
font-weight:bold;
text-align:center;
text-shadow:1px 1px 0px #ffffff;
}
.cal-content-nav li a:hover{
background:-webkit-linear-gradient(top, #ffffff 0%, #f5f5f5 100%);
background:-moz-linear-gradient(top, #ffffff 0%, #f5f5f5 100%);
text-decoration:none;
}
.cal-content-nav li.day a{
border-left:1px solid #c2c2c2;
}
.cal-content-nav li.day{
border-top-left-radius:3px;
border-bottom-left-radius:3px;
}
.cal-content-nav li.monthly{
border-top-right-radius:3px;
border-bottom-right-radius:3px;
}
.cal-content-nav li.acurrent{
border-bottom:1px solid #838383;
}

.cal-content-nav li.acurrent a{
border-right:1px solid #6e6e6e;
border-left:1px solid #6e6e6e;
color:#ffffff;
text-shadow:none;
background:#6e6e6e;
background:-webkit-linear-gradient(top, #5f5f5f 0%, #838383 100%);
background:-moz-linear-gradient(top, #5f5f5f 0%, #838383 100%);
}

.cal-content-nav li.weekly.acurrent a{
border-right:1px solid #6e6e6e;
}


